algoritmo "ParOuImpar"

var
   N: Inteiro
   Procedimento ParOuImpar(V: Inteiro)
      Inicio
      Se (V%2=0) entao
      Escreva("O número ",v," é par!!!")
      Senao
      Escreva("O número ",v," é impar!!!" )
      FimSe
      FimProcedimento
inicio
      Escreva("Digite um número: ")
      Leia(N)
      ParOuImpar(N)
fimalgoritmo
