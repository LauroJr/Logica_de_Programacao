algoritmo "FazSomaProcedimento"

var
   X, Y, N1, N2: Inteiro
Procedimento Soma(A, B: Inteiro)
Inicio
      Escreval("Recebi o valor ",A)
      Escreval("Recebi o vaor ",B)
      Escreval("O resultado da soma é: ",A+B)
FimProcedimento
inicio
      Escreva("1º número: ")
      Leia(N1)
      Escreva("2º número: ")
      Leia(N2)
      X <- N1
      Y <- N2
      Soma(X, Y)

fimalgoritmo
